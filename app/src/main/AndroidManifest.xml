<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="de.reneruck.inear2">

    <application
        android:name=".AppContext"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PlayActivity"></activity>
        <activity android:name=".settings.SettingsActivity"></activity>
        <service android:name=".service.PlaybackService"></service>
    </application>

</manifest>
